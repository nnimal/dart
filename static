class StaticExample {
  //fields
  static int addNumber = 0;
  static int subNumber = 0;

  //static method
  static void add(int a, int b) {
    print(a + b);
    addNumber++;
  }
}

void main() {
  StaticExample.add(45, 56);
  print(StaticExample.addNumber);
  StaticExample.add(459, 56);
  print(StaticExample.addNumber);
}
